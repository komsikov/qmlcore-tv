{
	"id": "com.{{ title }}.app",
	"version": "1.0.0",
	"vendor": "{{ author.name | default('PureQML Team') }}",
	"type": "web",
	"main": "index.html",
	"title": "{{ title }}",
	"bgColor": "{{ iconColor | default('#cccccc') }}",
	"bgImage": "preview.png",
	"icon": "icon.png",
	"largeIcon": "icon.png",
	"resolution":"{{ resolutionWidth | default(1280) }}x{{ resolutionHeight | default(720) }}",
	"iconColor": "{{ iconColor | default('#c8e6c9') }}",
	"splashBackground": "splash.png",
	"disableBackHistoryAPI":true,
	"uiRevision":1
}
